{
    "name": "nfephp-org/sped-gnre",
    "type": "library",
    "description": "API para a emissÃ£o de guias GNRE para a SEFAZ RJ",
    "keywords": ["nfe","nfce","sped","nfephp", "gnre"],
    "homepage": "https://github.com/Focus599Dev/sped-gnre-js/edit/master/composer.json",
    "license": ["GPL-3.0+","LGPL-3.0+", "MIT"],
    "authors": [
        {
            "name": "Marlon Oliveira Barbosa",
            "email": "marlon.academi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "nfephp-org/sped-common" : "^5.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "NFePHP\\GnreRJ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\GnreRJ\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "phpcbf": "vendor/bin/phpcbf --standard=psr2 src; vendor/bin/phpcbf --standard=psr2 tests",
        "phpcs": "vendor/bin/phpcs --standard=psr2 src; vendor/bin/phpcs --standard=psr2 tests"
    },
    "extra": {
        "branch-alias": {
            "v5.0": "5.0-dev"
        }
    },
    "minimum-stability": "stable"
}
